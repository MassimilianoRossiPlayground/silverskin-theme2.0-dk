    "main": {
      "type": "main-product",
      "settings": {
        "image_position": "left",
        "image_size": "medium",
        "thumbnail_position": "beside",
        "thumbnail_arrows": false,
        "description_style": "default",
        "social_enable": true
      }
    },
    "gallery": {
      "type": "product-gallery",
      "disabled": true,
